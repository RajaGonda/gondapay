{
  "name": "rajagonda/gondapay",
  "description": "The Laravel 5 Package for Indian Payment Gateways. Currently supported gateways: CCAvenue, PayUMoney, EBS, CitrusPay, InstaMojo, ZapakPay, Mocker",
  "license": "MIT",
  "keywords": [
    "Laravel 5",
    "Indian Payment Gateways",
    "CCAvenue",
    "PayUMoney",
    "EBS",
    "CitrusPay",
    "InstaMojo",
    "Mobikwik/ZapakPay",
    "Mocker"
  ],
  "authors": [
    {
      "name": "rajagonda",
      "email": "rajagonda@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "~5.0|~6.0",
    "guzzlehttp/guzzle": "~5.0|~6.0"
  },
  "autoload": {
    "psr-4": {
      "Rajagonda\\Gondapay\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rajagonda\\Gondapay\\GondapayServiceProvider"
      ],
      "aliases": {
        "Gondapay": "Rajagonda\\Gondapay\\Facades\\Gondapay"
      }
    }
  }
}
